body{
	margin:0;
	background-color:#f0f0f0;
}

.noData{
	font-size:14px;
	text-align:center;
	color:#333;
	margin-top:10px;
}

.scroller .loading2 {
     height: 40px;
     line-height: 40px;
     text-align: center;
     width: 100%;
     color: #555555;
     font-weight: bold;
}

.loadingMore{
	 height: 30px;
     line-height: 30px;
     text-align: center;
     width: 100%;
     color: #555555;
     font-weight: bold;
}

.row-message{
	width:100%;
	background-color:#fff;
	margin-bottom:5px;
}

.rowLine{
	width:100%;
	display:flex;
	background-color:#fff;
}

.topStyle{
	width: 100%;
    margin: 6px 3% 0px 3%;
    border-bottom:1px solid #f0f0f0;
}

.titleStyle{
	width:100%;
	font-size:16px;
	color:#333;
	display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow:hidden;
     word-break: break-all;
}

.tipStyle{
	padding:10px 0px;
	width:100%;
	display:flex;
	
}

.tipContain{
	flex:1;
	width:0;
}

.leftStyle{
	width:50%;
	text-align:left;
	font-size:14px;
	float:left;
	color:#949393;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}

.rightStyle{
	width:50%;
	text-align:right;
	font-size:14px;
	float:left;
	color:#949393;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}

.bottomStyle{
	width:100%;
	display:flex;
}

.countStyle{	
	width:25%;
	height:60px;
	position:relative;
}

.totalNum{
    position: absolute;
    left: 0;
    right: 0;
    margin: 0 auto;   
    color:#5f9fe8;
	font-size:14px;
	text-align:center;
	margin-top: 12px;
	font-weight:bold;
}

