@weuiBtnPlainPrimaryColor:@color-blue1;
@weuiBtnPlainPrimaryBorderColor:darken(@color-blue1,10%);
@weuiBtnPlainPrimaryActiveColor:lighten(@color-blue1,20%);
@weuiBtnPlainPrimaryActiveBorderColor:lighten(@color-blue1,10%);



@weuiBtnPlainDefaultColor:rgba(53,53,53,1);
@weuiBtnPlainDefaultBorderColor:rgba(53,53,53,1);
@weuiBtnPlainDefaultActiveColor:rgba(53,53,53,.6);
@weuiBtnPlainDefaultActiveBorderColor:rgba(53,53,53,.6);
